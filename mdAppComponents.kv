###    Default Widget Properties    ###
#######################################
#:import Snackbar kivymd.uix.snackbar.Snackbar
<MDTextField>
    font_size: '25sp'
    #hint_font_size: '100sp'
    #helper_font_size: '100sp'
    mode: 'round'
    fill_color_normal: "white"
    fill_color_focus: "white"
    line_color_normal: app.theme_cls.primary_color
    text_color_normal: app.theme_cls.primary_color
    helper_text_color_normal: app.theme_cls.primary_color 
    hint_text_color_normal: app.theme_cls.primary_color
    hint_text_color_focus: app.theme_cls.primary_color if app.theme_cls.theme_style == "Dark" else "black"
    text_color_focus: app.theme_cls.primary_color if app.theme_cls.theme_style == "Dark" else "black"


<CustomPasswordField>
    size_hint_y: None
    height: text_field.height
    pass_text: text_field
    MDTextField:
        id: text_field
        hint_text: root.hint_text
        text: root.text
        password: True
        required: True
          

    MDIconButton:
        id: password_open_icon
        icon: "eye-off"
        pos_hint: {'x': 0,, 'y': }
        pos_hint: {"center_y": .5}
        pos: text_field.width - self.width + dp(8), 0
        theme_text_color: "Custom"
        text_color: app.theme_cls.primary_color
        on_release:
            self.icon = "eye" if self.icon == "eye-off" else "eye-off"
            text_field.password = False if text_field.password is True else True 

<CustomPasswordRegField>
    size_hint_y: None
    height: password_field.height
    pass_text: password_field
    MDTextField:
        id: password_field
        hint_text: root.hint_text
        text: root.text
        font_size: "18sp"
        password: True
        required: True
          

    MDIconButton:
        id: password_open_icon
        icon: "eye-off"
        pos_hint: {"center_y": .5}
        pos: password_field.width - self.width + dp(8), 0
        theme_text_color: "Custom"
        text_color: app.theme_cls.primary_color
        on_release:
            self.icon = "eye" if self.icon == "eye-off" else "eye-off"
            password_field.password = False if password_field.password is True else True 


<RegDialogContent>
    orientation: "vertical"
    spacing: "16dp"
    padding: "16dp"
    size_hint_y: None
    height: "240dp"

    MDIcon:
        icon: 'check-circle-outline'
        theme_text_color: "Custom"
        text_color: 'green'
        font_size: '80sp'
        pos_hint: {'center_x': 0.5}
    MDLabel:
        text: "Registration Successful"
        bold: True
        font_style: 'H5'
        theme_text_color: 'Custom'
        text_color: 'orange'
        halign: "center"
    MDLabel:
        text: "You can now login using your registration information"
        bold: True
        font_style: 'Subtitle2'
        theme_text_color: 'Custom'
        text_color: 'gray'
        halign: "center"

<CustomSnackbar>
    MDIconButton:
        pos_hint: {'center_y': .5}
        icon: root.icon
        opposite_colors: True
    MDLabel:
        id: text_bar
        size_hint_y: None
        height: self.texture_size[1]
        text: root.text
        font_size: root.font_size
        theme_text_color: 'Custom'
        text_color: 'ffffff'
        shorten: True
        shorten_from: 'right'
        pos_hint: {'center_y': .5}
    
            
            
